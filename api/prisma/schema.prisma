// This is a Prisma schema file

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Case {
  id          Int      @id @default(autoincrement())
  caseNumber  String   @unique
  title       String
  description String
  status      String
  createdDate DateTime @default(now())
  dueDate     DateTime?
}
